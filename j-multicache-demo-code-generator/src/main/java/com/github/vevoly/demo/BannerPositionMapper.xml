<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.vevoly.demo.mapper.BannerPositionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.vevoly.demo.entity.BannerPosition">
        <id column="id" property="id" />
        <result column="banner_id" property="bannerId" />
        <result column="position_code" property="positionCode" />
        <result column="image_url" property="imageUrl" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, banner_id, position_code, image_url
    </sql>

</mapper>
