<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.vevoly.demo.mapper.UserLevelMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.vevoly.demo.entity.UserLevel">
        <id column="id" property="id" />
        <result column="tenant_id" property="tenantId" />
        <result column="level_name" property="levelName" />
        <result column="level_icon" property="levelIcon" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, tenant_id, level_name, level_icon
    </sql>

</mapper>
