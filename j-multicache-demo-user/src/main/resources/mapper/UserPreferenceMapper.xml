<mapper namespace="com.github.vevoly.demo.mapper.UserPreferenceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.vevoly.demo.entity.UserPreference">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="pref_key" property="prefKey" />
        <result column="pref_value" property="prefValue" />
